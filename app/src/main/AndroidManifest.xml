<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.prashant">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher_foreground"
        android:label="#task2"
        android:roundIcon="@drawable/ic_launcher_foreground"
        android:supportsRtl="true"
        android:theme="@style/Theme.prashant">
        <activity android:name="com.example.prashant.Activities.TransactionHistory"></activity>
        <activity android:name="com.example.prashant.Activities.SendToUserList" />
        <activity android:name="com.example.prashant.Activities.UserData" />
        <activity android:name="com.example.prashant.Activities.UsersList" />
        <activity android:name="com.example.prashant.Activities.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>